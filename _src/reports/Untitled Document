SELECT DISTINCT
     "PRODUCTS"."PRODUCTVENDOR",
     "DIM_TIME"."MONTH_ID",
     "DIM_TIME"."MONTH_DESC",
     sum("ORDERFACT"."TOTALPRICE") AS TOTALPRICE
FROM
     "PRODUCTS" INNER JOIN "ORDERFACT" ON "PRODUCTS"."PRODUCTCODE" = "ORDERFACT"."PRODUCTCODE"
     INNER JOIN "DIM_TIME" ON "ORDERFACT"."MONTH_ID" = "DIM_TIME"."MONTH_ID"
WHERE
     "ORDERFACT"."QTR_ID" = 1
GROUP BY
     "PRODUCTS"."PRODUCTVENDOR",
     "DIM_TIME"."MONTH_ID",
     "DIM_TIME"."MONTH_DESC"
ORDER BY
     "PRODUCTS"."PRODUCTVENDOR" ASC,
     "DIM_TIME"."MONTH_ID" ASC,
     "DIM_TIME"."MONTH_DESC" ASC
